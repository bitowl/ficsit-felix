    "now-build": "yarn build",
    "postbuild": "./rebuild-commithash.sh develop",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "prebuild": "./rebuild-commithash.sh build",
    "preelectron:build": "./rebuild-commithash.sh build",
    "postelectron:build": "./rebuild-commithash.sh develop"
