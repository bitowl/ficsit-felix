{
  "version": 2,
  "name": "ficsit-felix",
  "builds": [
    {"src": "app/package.json", "use": "@now/static-build",
    
    "config": {
    "distDir": "dist"
    }}
  ],
   "routes": [
      {"src": "/service-worker.js", "headers": { "cache-control": "s-maxage=0" } },
      { "handle": "filesystem" },
      {"src" : "/(.*)", "dest" :"/app/$1"},
      {"src": ".*", "dest": "/index.html"}
    ],
    "github": {
        "silent": true
    }
}

